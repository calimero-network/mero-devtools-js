#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/@near-wallet-selector+modal-ui@8.10.2_near-api-js@3.0.4/node_modules/qrcode/bin/node_modules:/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/@near-wallet-selector+modal-ui@8.10.2_near-api-js@3.0.4/node_modules/qrcode/node_modules:/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/@near-wallet-selector+modal-ui@8.10.2_near-api-js@3.0.4/node_modules:/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/@near-wallet-selector+modal-ui@8.10.2_near-api-js@3.0.4/node_modules/qrcode/bin/node_modules:/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/@near-wallet-selector+modal-ui@8.10.2_near-api-js@3.0.4/node_modules/qrcode/node_modules:/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/@near-wallet-selector+modal-ui@8.10.2_near-api-js@3.0.4/node_modules:/Users/xilosada/dev/calimero/calimero-abi-codegen/codegen-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../qrcode/bin/qrcode" "$@"
else
  exec node  "$basedir/../../../../qrcode/bin/qrcode" "$@"
fi
