#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules/@calimero-network/calimero-client/lib/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules/@calimero-network/calimero-client/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules/@calimero-network/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules/@calimero-network/calimero-client/lib/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules/@calimero-network/calimero-client/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules/@calimero-network/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/@calimero-network+calimero-client@file+.._@near-wallet-selector+modal-ui@8.10.2_near-ap_86dca43b2e96a1eb7c9568d9e8e8f7b5/node_modules:/Users/xilosada/dev/calimero/calimero-client-js/codegen-example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@calimero-network/calimero-client/lib/cli.cjs" "$@"
else
  exec node  "$basedir/../@calimero-network/calimero-client/lib/cli.cjs" "$@"
fi
